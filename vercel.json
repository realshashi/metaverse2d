{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "JWT_SECRET": "@JWT_SECRET"
  },
  "settings": {
    "memory": 3008,
    "maxDuration": 10
  }
}
